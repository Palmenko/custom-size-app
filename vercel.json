{
  "framework": "remix",
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install",
  "functions": {
    "app/routes/public-test.jsx": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/public-test",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
} 